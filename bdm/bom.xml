<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<businessObjectModel xmlns="http://documentation.bonitasoft.com/bdm-xml-schema/1.0" modelVersion="1.0" productVersion="7.13.0">
    <businessObjects>
        <businessObject qualifiedName="com.bonita.model.OdgovorNaZahtev">
            <description>Entitet koji predstavlja odgovor agenta firmi na zahtev kupca.</description>
            <fields>
                <field type="BOOLEAN" length="255" name="odustajanje" nullable="false" collection="false"/>
                <field type="DOUBLE" length="255" name="cena" nullable="false" collection="false"/>
                <field type="LOCALDATE" length="255" name="vremenskiRokZaIzvrsavanje" nullable="false" collection="false"/>
            </fields>
            <uniqueConstraints/>
            <queries/>
            <indexes/>
        </businessObject>
        <businessObject qualifiedName="com.bonita.model.PosaoIzvrsen">
            <description>Entitet koji se odnosi na to da li je posao zavrsen i da se daju ocene.</description>
            <fields>
                <field type="BOOLEAN" length="255" name="posaoIzvrsen" nullable="false" collection="false"/>
                <field type="DOUBLE" length="255" name="ocena" nullable="false" collection="false"/>
            </fields>
            <uniqueConstraints/>
            <queries/>
            <indexes/>
        </businessObject>
        <businessObject qualifiedName="com.bonita.model.PrihvatanjePonude">
            <description>Entitet koji sluzi kupcu za prihvatanje ponude firme i dodatne informacije.</description>
            <fields>
                <field type="BOOLEAN" length="255" name="prihvatanjePonude" nullable="false" collection="false"/>
                <field type="TEXT" length="255" name="dodatneInformacije" nullable="false" collection="false"/>
            </fields>
            <uniqueConstraints/>
            <queries/>
            <indexes/>
        </businessObject>
        <businessObject qualifiedName="com.bonita.model.Registracija">
            <description>Entitet za registraciju fizickih/pravnih lica.</description>
            <fields>
                <field type="STRING" length="255" name="ime" nullable="false" collection="false"/>
                <field type="STRING" length="255" name="email" nullable="false" collection="false"/>
                <field type="STRING" length="255" name="username" nullable="false" collection="false"/>
                <field type="STRING" length="255" name="password" nullable="false" collection="false"/>
                <field type="STRING" length="255" name="adresa" nullable="false" collection="false"/>
                <field type="STRING" length="255" name="mesto" nullable="false" collection="false"/>
                <field type="LONG" length="255" name="postanskiBroj" nullable="false" collection="false"/>
                <field type="BOOLEAN" length="255" name="firma" nullable="false" collection="false">
                    <description>u zavisnosti da li je firma biralo bi se koju kategoriju posla ona obavlja.</description>
                </field>
            </fields>
            <uniqueConstraints/>
            <queries/>
            <indexes/>
        </businessObject>
        <businessObject qualifiedName="com.bonita.model.SifarnikKategorijaPosla">
            <fields>
                <field type="STRING" length="255" name="kategorijaPosla" nullable="false" collection="false"/>
                <field type="TEXT" length="255" name="opis" nullable="false" collection="false"/>
            </fields>
            <uniqueConstraints/>
            <queries/>
            <indexes/>
        </businessObject>
        <businessObject qualifiedName="com.bonita.model.ZahtevKupca">
            <description>Entitet koji predstavlja zahtev kupca vezan za kategoriju posla(usluga) koja mu treba.</description>
            <fields>
                <relationField type="AGGREGATION" reference="com.bonita.model.SifarnikKategorijaPosla" fetchType="LAZY" name="kategorijaPoslova" nullable="false" collection="false"/>
                <field type="TEXT" length="255" name="opisPosla" nullable="false" collection="false"/>
                <field type="DOUBLE" length="255" name="vrednost" nullable="false" collection="false"/>
                <field type="LOCALDATE" length="255" name="rokZaPonude" nullable="false" collection="false"/>
                <field type="INTEGER" length="255" name="brojOcekivanihPonuda" nullable="false" collection="false"/>
                <field type="LOCALDATE" length="255" name="rokZaIzvrsavanje" nullable="false" collection="false"/>
            </fields>
            <uniqueConstraints/>
            <queries/>
            <indexes/>
        </businessObject>
    </businessObjects>
</businessObjectModel>
